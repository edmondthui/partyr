{
  "name": "partyr",
  "version": "1.0.0",
  "description": "MERN stack application to help organize parties",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js",
    "debug": "node --inspect app.js",
    "frontend-install": "npm install --prefix frontend",
    "frontend": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run frontend\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "author": "Edmond Hui, Jasmine Lok, Alexandria Wong, Ray Liang",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "aws-sdk": "^2.802.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "google-maps-react": "^2.0.6",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.16",
    "multer": "^1.4.2",
    "node-sass": "^4.14.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "socket.io": "^3.0.3",
    "validator": "^13.1.17"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
}
